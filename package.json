{
  "name": "@jeremyling/react-material-ui-form-builder",
  "version": "0.4.2",
  "scripts": {
    "start": "react-scripts start",
    "build": "NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src --out-dir dist --copy-files --no-comments",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "An easy-to-use and quick form builder using the following React Material UI input components:",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "babel": {
    "presets": [
      "react",
      "env",
      "stage-2",
      "minify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jzcling/react-material-ui-form-builder.git"
  },
  "keywords": [
    "form",
    "builder",
    "form builder",
    "material ui",
    "react"
  ],
  "author": "Jeremy Ling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jzcling/react-material-ui-form-builder/issues"
  },
  "homepage": "https://github.com/jzcling/react-material-ui-form-builder#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.3"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@jeremyling/react-material-ui-rich-text-editor": "^0.1.4",
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.3.10",
    "lodash": "^4.17.21",
    "react-player": "^2.9.0",
    "yup": "^0.32.9"
  }
}
